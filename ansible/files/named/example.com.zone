$TTL 86400
@       IN SOA example.com. example.com(
                            2018010901      ; Serial
                            10800           ; Refresh
                            3600            ; Retry
                            3600000         ; Expire
                            3600            ; Negative Cashe TTL
)
        IN NS example.com.
@       IN MX 10 mail.example.com.
@       IN TXT "v=spf1 ip4:192.168.33.10 -all" ; SPF record
@       IN A  192.168.33.10
